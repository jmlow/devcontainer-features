{
	"name": "mise-en-place",
	"id": "mise",
	"version": "1.0.0",
	"description": "A mise.jdx.dev feature",
	"options": {
		"debug": {
			"type": "boolean",
			"default": false,
			"description": "Enable debug logging"
		},
		"quiet": {
			"type": "boolean",
			"default": true,
			"description": "Disable non-error output"
		},
		"install_path": {
			"type": "string",
			"default": "~/.local/bin/mise",
			"description": "Change the binary path (default: '~/.local/bin/mise')"
		},
		"version": {
			"type": "string",
			"default": "",
			"description": "Install a specific version"
		}
	},
	"installsAfter": ["ghcr.io/devcontainers/features/common-utils"]
}
