{
	"name": "lazyvim",
	"id": "lazyvim",
	"version": "0.1.3",
	"description": "A lazyvim feature",
	"dependsOn": {
		"ghcr.io/jmlow/devcontainer-features/curl:0": {},
		"ghcr.io/jmlow/devcontainer-features/wget:0": {},
		"ghcr.io/jmlow/devcontainer-features/unzip:0": {},
		"ghcr.io/jmlow/devcontainer-features/ripgrep:0": {},
		"ghcr.io/jmlow/devcontainer-features/fdfind:0": {},
		"ghcr.io/jmlow/devcontainer-features/fzf:0": {},
		"ghcr.io/jmlow/devcontainer-features/lsof:0": {},
		"ghcr.io/jmlow/devcontainer-features/tmux:0": {},
		"ghcr.io/jmlow/devcontainer-features/lazygit:0": {},
		"ghcr.io/devcontainers-extra/features/mise:latest": {},
		"ghcr.io/jmlow/devcontainer-features/pynvim:0": {}
	},
	"updateContentCommand": [
		"mise use --env lazyvim python@latest",
		"mise use --env lazyvim node@lts",
		"mise use --env lazyvim rust@latest",
		"cargo install tree-sitter-cli zellij",
		"npm i -g neovim @github/copilot"
	]
}