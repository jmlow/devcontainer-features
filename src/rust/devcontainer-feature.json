{
	"name": "rust",
	"id": "rust",
	"version": "0.1.1",
	"description": "Installs Rust toolchain via rustup with cargo-binstall",
	"containerEnv": {
		"CARGO_HOME": "/usr/local/cargo",
		"RUSTUP_HOME": "/usr/local/rustup",
		"PATH": "${CARGO_HOME}/bin:${PATH}"
	},
	"dependsOn": {
		"ghcr.io/devcontainers/features/common-utils:2": {}
	}
}
